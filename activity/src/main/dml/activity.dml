package pt.ulisboa.tecnico.softeng.activity.domain;

class ActivityProvider {
	String code;
	String name;
}

class Activity {
	String code;
	String name;
	int minAge;
	int maxAge;
	int capacity;
}

class ActivityOffer {

}

relation DomainRootHasActivityProviderApplication {
	.pt.ist.fenixframework.DomainRoot playsRole root {
		multiplicity 1..1;
	}
	ActivityProvider playsRole activityProvider {
		multiplicity 0..*;
	}
}

relation ActivityProviderHasActivity {
	ActivityProvider playsRole activityProvider {
		multiplicity 1..1;
	}
	Activity playsRole activity {
		multiplicity 0..*;
	}
}